<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>nullable</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AtmaFileSystem" Version="5.0.0" />
    <PackageReference Include="Bullseye" Version="4.0.0" />
    <PackageReference Include="DotnetExeCommandLineBuilder" Version="0.3.0" />
    <PackageReference Include="FluentAssertions" Version="6.5.1" />
    <PackageReference Include="NScan" Version="0.90.1" />
    <PackageReference Include="NScan.Adapters.Secondary" Version="0.90.1" />
    <PackageReference Include="SimpleExec" Version="9.1.0" />
  </ItemGroup>

</Project>
